{
  "name": "4ndr0ip",
  "version": "3.3",
  "manifest_version": 3,
  "description": "Dynamically blocks IP grabbers and logs WebRTC leaks.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "4ndr0ip",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "scripting",
    "declarativeNetRequest",
    "privacy"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "initial_ruleset",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "icons": { "128": "icon_128.png" }
}
